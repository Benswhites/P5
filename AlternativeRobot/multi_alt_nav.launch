<launch>
  <param name="/use_sim_time" value="true"/>



  <group ns="robot1">
    <param name="tf_prefix" value="robot1_tf" />
    <param name="amcl/initial_pose_x" value="1" />
    <param name="amcl/initial_pose_y" value="1" />
    <include file="$(find multi_robot)/launch/WildBen/AlternativeRobot/move_base.launch" />
  </group>

  <group ns="robot2">
    <param name="tf_prefix" value="robot2_tf" />
    <param name="amcl/initial_pose_x" value="-1" />
    <param name="amcl/initial_pose_y" value="1" />
    <include file="$(find multi_robot)/launch/WildBen/AlternativeRobot/move_base.launch" />
  </group>

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find gazebo_worlds)/rviz/navigation.rviz"
   output="screen" />

</launch>