<launch>    
  <!--- Run AMCL -->
  <include file="$(find turtlebot3_navigation)/launch/amcl.launch" />

  <!-- Define your move_base node -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find multi_robot)/launch/WildBen/AlternativeRobot/costmap_common_params_waffle.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find multi_robot)/launch/WildBen/AlternativeRobot/costmap_common_params_waffle.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find multi_robot)/launch/WildBen/AlternativeRobot/local_costmap_params.yaml"  command="load" />
    <rosparam file="$(find multi_robot)/launch/WildBen/AlternativeRobot/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find multi_robot)/launch/WildBen/AlternativeRobot/base_local_planner_params.yaml" command="load" />

    <remap from="map" to="/map" />
  </node>
</launch>